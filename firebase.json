{
  "hosting": [
    {
      "target": "flippedlearn",
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "flipped-learn-front-staging",
            "region": "asia-southeast1"
          }
        }
      ]
    },
    {
      "target": "staging",
      "rewrites": [
        {
          "source": "api/**",
          "run": {
            "serviceId": "flipped-learn-back-staging",
            "region": "asia-southeast1"
          }
        },
        {
          "source": "**",
          "run": {
            "serviceId": "flipped-learn-front-staging",
            "region": "asia-southeast1"
          }
        }
      ]
    }
  ]
}
